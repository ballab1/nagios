#!/bin/bash


mkdir -p "${WWW}"
tar xf "${NCONFF['file']}" -C "${WWW}" 

rm -rf "${NCONFF['home']}/ADD-ONS"
rm -rf "${NCONFF['home']}/config.orig"
rm -rf "${NCONFF['home']}/INSTALL"*
rm -rf "${NCONFF['home']}/UPDATE"*
rm -rf "${NCONFF['home']}/SUMS"*
mkdir -p "${NCONFF['home']}/config"
mkdir -p "${NCONFF['home']}/output"
mkdir -p "${NCONFF['home']}/static_cfg"
mkdir -p "${NCONFF['home']}/temp"
