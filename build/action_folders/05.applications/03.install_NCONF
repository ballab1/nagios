#!/bin/bash


mkdir -p "${NCONF['www']}"

tar xf "${NCONF['file']}" -C "$WWW" 

#rm -rf "${NCONF['www']}/ADD-ONS"
rm -rf "${NCONF['www']}/config.orig"
rm -rf "${NCONF['www']}/INSTALL"*
rm -rf "${NCONF['www']}/UPDATE"*
#rm -rf "${NCONF['www']}/SUMS"*
mkdir -p "${NCONF['www']}/config"
mkdir -p "${NCONF['www']}/output"
mkdir -p "${NCONF['www']}/static_cfg"
mkdir -p "${NCONF['www']}/temp"
